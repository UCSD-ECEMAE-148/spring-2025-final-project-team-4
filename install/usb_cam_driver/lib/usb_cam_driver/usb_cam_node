#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'usb-cam-driver','console_scripts','usb_cam_node'
__requires__ = 'usb-cam-driver'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('usb-cam-driver', 'console_scripts', 'usb_cam_node')()
    )
